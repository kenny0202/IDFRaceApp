@model IEnumerable<IDFWebApp.Models.Custom.brackettemplate>
@if (Model.Count() == 0)
{
    <h4 style="color: #ff0000">Not Found</h4>
}
else
{
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BracketNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BracketSeed)
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.BracketNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BracketSeed)
                </td>
            </tr>
        }
    </table>
}
